{
  "name": "cached",
  "version": "4.0.2",
  "description": "Simple access to a cache",
  "main": "lib/cached.js",
  "keywords": [
    "memcached",
    "stampede",
    "cache"
  ],
  "scripts": {
    "build": "coffee -cbo lib src",
    "watch": "coffee -wcbo lib src & nodemon -w src -w test -e coffee,js,json -x mocha",
    "prepublish": "rm -rf lib && npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npub verify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/groupon/node-cached.git"
  },
  "author": {
    "name": "Jan Krems",
    "email": "jan.krems@groupon.com>"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "license": {
      "exclude": [
        "lib",
        "test"
      ]
    }
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "lodash": "^3.1.0",
    "memcached": "^2.1.0"
  },
  "devDependencies": {
    "assertive": "^1.4.1",
    "coffee-script": "^1.9.0",
    "mocha": "^2.0.1",
    "nodemon": "^1.8.0",
    "npub": "~0.5.1"
  }
}
